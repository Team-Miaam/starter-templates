		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.3",
		"prettier": "^2.4.1"
	},
	"dependencies": {
		"miaam-scripts": "^0.3.2"
